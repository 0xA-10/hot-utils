{
  "name": "hot-fns",
  "version": "0.1.3",
  "description": "Utilities optimized for hot paths - more efficient than lodash equivalents.",
  "keywords": [
    "utils",
    "utilities",
    "performance",
    "lodash",
    "hot",
    "hotpath",
    "hot path",
    "hot paths",
    "fast",
    "mapKeys",
    "mapObject",
    "map fast"
  ],
  "author": "Tony Natelli <tony@suspended.systems>",
  "license": "MIT",
  "scripts": {
    "build": "pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && node scripts/rename-cjs.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "gts lint",
    "fix": "gts fix",
    "clean": "rm -rf dist",
    "bench": "node --import tsx bench/run.ts",
    "bench:baseline": "node --import tsx bench/run.ts --save-baseline",
    "bench:ci": "node --import tsx bench/run.ts --ci",
    "prepack": "pnpm build"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "gts": "^7.0.0",
    "tinybench": "^6.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.16"
  },
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./array": {
      "import": {
        "types": "./dist/esm/array/index.d.ts",
        "default": "./dist/esm/array/index.js"
      },
      "require": {
        "types": "./dist/cjs/array/index.d.cts",
        "default": "./dist/cjs/array/index.cjs"
      }
    },
    "./object": {
      "import": {
        "types": "./dist/esm/object/index.d.ts",
        "default": "./dist/esm/object/index.js"
      },
      "require": {
        "types": "./dist/cjs/object/index.d.cts",
        "default": "./dist/cjs/object/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  }
}
